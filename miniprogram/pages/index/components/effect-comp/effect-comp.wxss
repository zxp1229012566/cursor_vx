.page {
  background: #f7f8fa;
  min-height: 100vh;
  padding: 32rpx 24rpx 0 24rpx;
}
.loading-box {
  margin: 80rpx 0 40rpx 0;
  display: flex;
  justify-content: center;
}
.result-box {
  margin: 40rpx 0 40rpx 0;
}
.effect-img-wrap {
  display: flex;
  justify-content: center;
  margin-bottom: 24rpx;
}
.effect-img {
  width: 240rpx;
  height: 240rpx;
  border-radius: 32rpx;
  background: #f5e9da;
}
.btn-group {
  margin-top: 32rpx;
} 